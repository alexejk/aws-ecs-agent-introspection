{
  "Vendor": true,
  "EnableGC": false,
  "Aggregate": true,
  "Deadline": "30s",
  "Sort": [
    "linter",
    "severity",
    "path"
  ],
  "Exclude": [
    "_test.go"
  ],
  "Skip": [
      "testData/",
      "vendor/"
  ],
  "Enable": [
    "deadcode",
    "gofmt",
    "goimports",
    "golint",
    "gosimple",
    "ineffassign",
    "interfacer",
    "unconvert",
    "unused",
    "errcheck",
    "vet"
  ],
  "LineLength": 200
}
